DISCORD_TOKEN=your_bot_token_here

# Optional: For instant slash-command sync while developing.
# If set, commands sync to this single guild instead of globally.
# COMMAND_GUILD_ID=123456789012345678

# Optional: queue channel where the panel lives.
# QUEUE_CHANNEL_ID=123456789012345678
# MODMAIL_CHANNEL_ID=123456789012345678
# MODMAIL_LOGS_CHANNEL_ID=123456789012345678
# LEADERBOARD_CHANNEL_ID=123456789012345678
# MAIN_VOICE_CHANNEL_ID=123456789012345678
# TEAM_A_VOICE_CHANNEL_ID=123456789012345678
# TEAM_B_VOICE_CHANNEL_ID=123456789012345678

# Optional queue defaults
# SQLITE_PATH=bot.db
# PLAYERS_PER_MATCH=10
# TANK_PER_TEAM=1        # legacy; ignored by queue flow
# DPS_PER_TEAM=2         # legacy; ignored by queue flow
# SUPPORT_PER_TEAM=2     # legacy; ignored by queue flow

# Backward-compatible alias for PLAYERS_PER_MATCH
# QUEUE_SIZE=10

# Optional profile defaults
# DEFAULT_MMR=2500
# DEFAULT_ROLE=queue
